{
  "name": "fake-element-ui",
  "version": "1.0.0",
  "description": "fake element ui",
  "main": "index.js",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.5.2"
  },
  "scripts": {
    "build:file": "node build/bin/iconInit.js & node build/bin/build-entry.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage",
    "dev": "yarn run build:file && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js & node build/bin/template.js",
    "dist": "npm run clean && npm run build:file && npm run lint && webpack --config build/webpack.component.js",
    "dist:umd": "npm run clean && npm run lint && webpack --config build/webpack.component.umd.js",
    "lint": "eslint src/**/* packages/**/* build/**/* --quiet"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "4.14.0",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-json": "^1.2.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-templater": "^1.2.0",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-salad": "^2.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.1",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-node-externals": "^1.7.2"
  }
}
